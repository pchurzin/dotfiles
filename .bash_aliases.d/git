alias ga='git add'
alias gb='git branch'
alias gc='git commit'
alias gca='git commit --amend --no-edit'
alias gd='git diff'
alias gds='git diff --staged'
alias gfo='git fetch origin --prune'
alias gh="git log --pretty=format:'%C(auto)%h %ad | %s [%an]%d%Creset' --graph --all --date=short"
alias ghd='git log --oneline develop..'
alias gk='git checkout'
alias go='git switch'
alias gp='git push'
alias gr='git rebase'
alias gra='git rebase --abort'
alias grc='git rebase --continue'
alias grd='git rebase develop'
alias gri='git rebase -i'
alias grid='git rebase -i develop'
alias gs='git status'
alias gud='git fetch origin develop:develop'
alias gw='git stash push -u'
alias gW='git stash pop'
